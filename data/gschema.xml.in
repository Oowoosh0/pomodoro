<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema path="/com/github/oowoosh0/pomodoro/" id=@APP_ID@>
    <key name="work-time-minutes" type="i">
      <default>25</default>
      <summary>Duration of work time in minutes</summary>
      <description>Duration of work time in minutes</description>
    </key>
    <key name="break-time-minutes" type="i">
      <default>5</default>
      <summary>Duration of break time in minutes</summary>
      <description>Duration of break time in minutes</description>
    </key>
    <key name="long-break-time-minutes" type="i">
      <default>15</default>
      <summary>Duration of long break time in minutes</summary>
      <description>Duration of long break time in minutes</description>
    </key>
    <key name="intervals-to-long-break" type="i">
      <default>4</default>
      <summary>Number of work intervals before a long break</summary>
      <description>Number of work intervals before a long break</description>
    </key>
    <key name="autostart-interval" type="b">
      <default>false</default>
      <summary>Autostart the next interval</summary>
      <description>Autostarts the next interval as soon as the current interval ends</description>
    </key>
    <key name="show-notifications" type="b">
      <default>true</default>
      <summary>Show notifiaction when interval ends</summary>
      <description>Show notifiaction when interval ends</description>
    </key>
    <key name="raise-window" type="b">
      <default>false</default>
      <summary>Raise window to the top when interval ends</summary>
      <description>Raise window to the top when interval ends</description>
    </key>
  </schema>
</schemalist>
